{
  "name": "Chinolor Light",
  "type": "light",
  "semanticHighlighting": true,
  "colors": {
    "editor.background": "#f8f4ed",
    "editor.foreground": "#2b312c",
    "sideBarTitle.foreground": "#2b312c",
    "scrollbar.shadow": "#a4aca7",
    "editorCursor.foreground": "#2b312c",
    "editor.selectionBackground": "#c7d2d4",
    "editor.wordHighlightBackground": "#c7d2d4",
    "editor.wordHighlightStrongBackground": "#c7d2d4",
    "editor.lineHighlightBackground": "#e7e3db",
    "editor.lineHighlightBorder": "#e7e3db",
    "editor.findMatchBackground": "#d3ccccee",
    "editor.findMatchHighlightBackground": "#f8b37f55",
    "editor.rangeHighlightBackground": "#f8e8c188",
    "editorIndentGuide.activeBackground": "#8a988e",
    "editorBracketHighlight.foreground1": "#b78d12",
    "editorBracketHighlight.foreground2": "#873680",
    "editorBracketHighlight.foreground3": "#179ab5",
    "editorBracketHighlight.unexpectedBracket.foreground": "#ee3f4d",
    "activityBar.background": "#f3efe8",
    "activityBar.foreground": "#2b312c",
    "activityBar.activeBorder": "#2b312c",
    "sideBar.background": "#eee8e0",
    "statusBar.background": "#f8e8c1",
    "statusBar.foreground": "#2b312c",
    "statusBar.noFolderBackground": "#d1c2d3",
    "statusBar.debuggingBackground": "#f8b37f",
    "minimap.selectionHighlight": "#c7d2d4",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBlue": "#619ac3",
    "terminal.ansiCyan": "#57c3c2",
    "terminal.ansiGreen": "#96c24e",
    "terminal.ansiMagenta": "#d276a3",
    "terminal.ansiRed": "#ee3f4d",
    "terminal.ansiWhite": "#e4dfd7",
    "terminal.ansiYellow": "#f9d367",
    "terminal.ansiBrightBlack": "#2b312c",
    "terminal.ansiBrightBlue": "#619ac3",
    "terminal.ansiBrightCyan": "#12aa9c",
    "terminal.ansiBrightGreen": "#41ae3c",
    "terminal.ansiBrightMagenta": "#cc5595",
    "terminal.ansiBrightRed": "#ee3f4d",
    "terminal.ansiBrightWhite": "#f8f4ed",
    "terminal.ansiBrightYellow": "#b78d12",
    "list.activeSelectionBackground": "#0066BF",
    "list.focusHighlightForeground": "#9DDDFF",
    "statusBarItem.prominentBackground": "#7b7b7b62"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#a4aca7"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#2b312c"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ee3f4d"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#cc5595"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "keyword.other.template",
        "keyword.other.substitution",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#12aa9c"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "entity.name.method",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#1781b5"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#fc6315"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#b78d12"
      }
    },
    {
      "name": "Class, Support",
      "scope": ["entity.name"],
      "settings": {
        "foreground": "#1781b5"
      }
    },
    {
      "scope": ["entity.name.type"],
      "settings": {
        "foreground": "#41ae3c",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["entity.name.tag", "entity.name.section"],
      "settings": {
        "foreground": "#ed5a65"
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter"
      ],
      "settings": {
        "foreground": "#41ae3c"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "support.variable.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#ed5a65"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#1781b5"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module",
        "variable.import.parameter",
        "variable.other.class"
      ],
      "settings": {
        "foreground": "#ea517f"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ea517f"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#983680"
      }
    },
    {
      "name": "CSS ID",
      "scope": ["entity.other.attribute-name.id"],
      "settings": {
        "foreground": "#207f4c"
      }
    },
    {
      "name": "CSS !important",
      "scope": ["keyword.other.important"],
      "settings": {
        "foreground": "#ee3f4d"
      }
    },
    {
      "name": "CSS Media",
      "scope": ["support.type.property-name.media"],
      "settings": {
        "foreground": "#41ae3c"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#1781b5"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ee3f4d"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#983680"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#207f4c"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#207f4c"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#983680"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#1781b5"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#fc7930"
      }
    },
    {
      "name": "JSON Key - Level 3",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#41ae3c"
      }
    },
    {
      "name": "JSON Key - Level 4",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#ed5a65"
      }
    },
    {
      "name": "JSON Key - Level 5",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#619ac3"
      }
    },
    {
      "name": "Markdown - Plain",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": {
        "foreground": "#2b312c"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": [
        "text.html.markdown markup.inline.raw.markdown",
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "foreground": "#12aa9c"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline Punctuation",
      "scope": [
        "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
      ],
      "settings": {
        "foreground": "#8a988e"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#1781b5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ed5a65"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ed5a65"
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ed5a65"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#cc5595"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#983680",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#b78d12"
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": ["string.other.link.description.title.markdown"],
      "settings": {
        "foreground": "#983680"
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": ["constant.other.reference.link.markdown"],
      "settings": {
        "foreground": "#b78d12"
      }
    },
    {
      "name": "Markup - Raw Block",
      "scope": ["markup.raw.block"],
      "settings": {
        "foreground": "#983680"
      }
    },
    {
      "name": "Markdown - Raw Block Fenced",
      "scope": ["markup.raw.block.fenced.markdown"],
      "settings": {
        "foreground": "#00000050"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block",
      "scope": ["punctuation.definition.fenced.markdown"],
      "settings": {
        "foreground": "#00000050"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block Variable",
      "scope": [
        "markup.raw.block.fenced.markdown",
        "variable.language.fenced.markdown",
        "punctuation.section.class.end"
      ],
      "settings": {
        "foreground": "#2b312c"
      }
    },
    {
      "name": "Markdown - Fenced Language",
      "scope": ["variable.language.fenced.markdown"],
      "settings": {
        "foreground": "#8a988e"
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": ["meta.separator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#8a988e"
      }
    },
    {
      "name": "Markup - Table",
      "scope": ["markup.table"],
      "settings": {
        "foreground": "#2b312c"
      }
    },
    {
      "scope": ["beginning.punctuation.definition.list.markdown"],
      "settings": {
        "foreground": "#fc7930"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#1781b5"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#fc7930"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#ee3f4d"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#ed5a65"
      }
    }
  ]
}
